{
  "title": "Iesu Me Ke Kanaka Waiwai",
  "composer": "John K. Almeida",
  "translator": "Haunani Bernardino",
  "song_type": "bilingual",
  "sections": [
    {
      "id": "v1",
      "type": "verse",
      "number": 1,
      "order": 1,
      "lines": [
        {
          "id": "v1.1",
          "line_number": 1,
          "hawaiian_text": "Ma ke alahele ʻo Iesû",
          "english_text": "Along the road, Jesus",
          "is_bilingual": true
        },
        {
          "id": "v1.2", 
          "line_number": 2,
          "hawaiian_text": "I hālāwai aku ai",
          "english_text": "Met",
          "is_bilingual": true
        },
        {
          "id": "v1.3",
          "line_number": 3,
          "hawaiian_text": "Me ke kanaka ʻōpio hanohano",
          "english_text": "With a distinguished young man",
          "is_bilingual": true
        },
        {
          "id": "v1.4",
          "line_number": 4,
          "hawaiian_text": "Kaulana i ka waiwai",
          "english_text": "Who was known for his wealth",
          "is_bilingual": true
        },
        {
          "id": "v1.5",
          "line_number": 5,
          "hawaiian_text": "Pane mai e ka ʻōpio",
          "english_text": "The young man said,",
          "is_bilingual": true
        },
        {
          "id": "v1.6",
          "line_number": 6,
          "hawaiian_text": "ʻE kuʻu Haku maikaʻi",
          "english_text": "\"My good lord,",
          "is_bilingual": true
        },
        {
          "id": "v1.7",
          "line_number": 7,
          "hawaiian_text": "He aha hoʻi kaʻu e hana aku ai",
          "english_text": "What must I do",
          "is_bilingual": true
        },
        {
          "id": "v1.8",
          "line_number": 8,
          "hawaiian_text": "I loaʻa e ke ola mau?",
          "english_text": "To gain eternal life?\"",
          "is_bilingual": true
        }
      ]
    },
    {
      "id": "c1",
      "type": "chorus",
      "number": 1,
      "order": 2,
      "lines": [
        {
          "id": "c1.1",
          "line_number": 1,
          "hawaiian_text": "E hāʻawi, e hāʻawi lilo",
          "english_text": "\"Give, give away all",
          "is_bilingual": true
        },
        {
          "id": "c1.2",
          "line_number": 2,
          "hawaiian_text": "I kou mau waiwai",
          "english_text": "Of your possessions",
          "is_bilingual": true
        },
        {
          "id": "c1.3",
          "line_number": 3,
          "hawaiian_text": "Huli a hahai mai iaʻu",
          "english_text": "Then come and follow me",
          "is_bilingual": true
        },
        {
          "id": "c1.4",
          "line_number": 4,
          "hawaiian_text": "I loaʻa e ke ola mau ia ʻoe",
          "english_text": "In order to gain eternal life\"",
          "is_bilingual": true
        }
      ]
    },
    {
      "id": "v2",
      "type": "verse", 
      "number": 2,
      "order": 3,
      "lines": [
        {
          "id": "v2.1",
          "line_number": 1,
          "hawaiian_text": "Minamina e ka ʻōpio",
          "english_text": "The young man grieved",
          "is_bilingual": true
        },
        {
          "id": "v2.2",
          "line_number": 2,
          "hawaiian_text": "I kona mau waiwai",
          "english_text": "Over his wealth",
          "is_bilingual": true
        },
        {
          "id": "v2.3",
          "line_number": 3,
          "hawaiian_text": "I ke kūʻai a hāʻawi lilo aku",
          "english_text": "Unwilling to sell and give all",
          "is_bilingual": true
        },
        {
          "id": "v2.4",
          "line_number": 4,
          "hawaiian_text": "I ka poʻe nele a hune",
          "english_text": "To the poor and destitute",
          "is_bilingual": true
        },
        {
          "id": "v2.5",
          "line_number": 5,
          "hawaiian_text": "Huli aʻe ʻo Iesū lā",
          "english_text": "Jesus then turned",
          "is_bilingual": true
        },
        {
          "id": "v2.6",
          "line_number": 6,
          "hawaiian_text": "Pane aku i ka ʻōpio",
          "english_text": "And answered the man,",
          "is_bilingual": true
        },
        {
          "id": "v2.7",
          "line_number": 7,
          "hawaiian_text": "ʻAʻole aʻe hiki ke kanaka waiwai",
          "english_text": "\"Rich man, you will not enter",
          "is_bilingual": true
        },
        {
          "id": "v2.8",
          "line_number": 8,
          "hawaiian_text": "I ke aupuni o ka lani",
          "english_text": "The Kingdom of Heaven\"",
          "is_bilingual": true
        }
      ]
    }
  ],
